import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sca = new Scanner(System.in);
        while(sca.hasNext()){
            int n = sca.nextInt();
            int m = sca.nextInt();
            int[] array = new int[n+1];
            for(int i = 0;i < m;i++){
                int temp = sca.nextInt();
                array[temp] += 1;
            }
            Arrays.sort(array);
            System.out.println(array[1]);
        }
    }
}
